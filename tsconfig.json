{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "sourceMap": true,
        "outDir": "build/",
        "removeComments": true,
        "alwaysStrict": true
    
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules"
    ],
    "typedocOptions": {
        "entryPointStrategy":"expand",
        "entryPoints":["./src"],
        "out": "docs"
    }
   
}


